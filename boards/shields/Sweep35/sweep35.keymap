#include <physical_layouts.dtsi>
#include <behaviors.dtsi>
#include <dt-bindings/zmk/bt.h>
#include <dt-bindings/zmk/keys.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
                // Row 1
                &kp_0   &kp_1   &kp_2   &kp_3   &kp_4   &kp_5   &kp_6   &kp_7   &kp_8   &kp_9
                // Row 2
                &kp_10  &kp_11  &kp_12  &kp_13  &kp_14  &kp_15  &kp_16  &kp_17  &kp_18  &kp_19
                // Row 3
                &kp_20  &kp_21  &kp_22  &kp_23  &kp_24  &kp_25  &kp_26  &kp_27  &kp_28  &kp_29
                // Thumbs (center 5 keys)
                &kp_30  &kp_31  &kp_32  &kp_33  &kp_34
            >;
        };

        bindings {
            kp_0  { label = "Q";   behavior = &kp KC_Q;   };
            kp_1  { label = "W";   behavior = &kp KC_W;   };
            kp_2  { label = "F";   behavior = &kp KC_F;   };
            kp_3  { label = "P";   behavior = &kp KC_P;   };
            kp_4  { label = "G";   behavior = &kp KC_G;   };
            kp_5  { label = "J";   behavior = &kp KC_J;   };
            kp_6  { label = "L";   behavior = &kp KC_L;   };
            kp_7  { label = "U";   behavior = &kp KC_U;   };
            kp_8  { label = "Y";   behavior = &kp KC_Y;   };
            kp_9  { label = ";";   behavior = &kp KC_SCLN; };

            kp_10 { label = "A";   behavior = &kp KC_A;   };
            kp_11 { label = "R";   behavior = &kp KC_R;   };
            kp_12 { label = "S";   behavior = &kp KC_S;   };
            kp_13 { label = "T";   behavior = &kp KC_T;   };
            kp_14 { label = "D";   behavior = &kp KC_D;   };
            kp_15 { label = "H";   behavior = &kp KC_H;   };
            kp_16 { label = "N";   behavior = &kp KC_N;   };
            kp_17 { label = "E";   behavior = &kp KC_E;   };
            kp_18 { label = "I";   behavior = &kp KC_I;   };
            kp_19 { label = "O";   behavior = &kp KC_O;   };

            kp_20 { label = "Z";   behavior = &kp KC_Z;   };
            kp_21 { label = "X";   behavior = &kp KC_X;   };
            kp_22 { label = "C";   behavior = &kp KC_C;   };
            kp_23 { label = "V";   behavior = &kp KC_V;   };
            kp_24 { label = "B";   behavior = &kp KC_B;   };
            kp_25 { label = "K";   behavior = &kp KC_K;   };
            kp_26 { label = "M";   behavior = &kp KC_M;   };
            kp_27 { label = ",";   behavior = &kp KC_COMM; };
            kp_28 { label = ".";   behavior = &kp KC_DOT;  };
            kp_29 { label = "/";   behavior = &kp KC_SLSH; };

            kp_30 { label = "GUI";     behavior = &kp KC_LGUI;  };
            kp_31 { label = "TAB";     behavior = &kp KC_TAB;   };
            kp_32 { label = "SPACE";   behavior = &kp KC_SPC;   };
            kp_33 { label = "ENTER";   behavior = &kp KC_ENT;   };
            kp_34 { label = "BACKSP";  behavior = &kp KC_BSPC;  };
        };
    };
};
