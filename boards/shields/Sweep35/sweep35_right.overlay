#include "sweep35.dtsi"

/ {
    chosen {
        zmk,keyboard = &sweep35_right;
    };

    sweep35_right: sweep35_right {
        compatible = "zmk,keyboard";
        label = "Sweep35 Right Half";

        matrix-transform = <&default_transform>;
        kscan = <&kscan0>;
        keymap = <&keymap_right>;
    };

    &default_transform {
        col-offset = <18>;
    };
};
